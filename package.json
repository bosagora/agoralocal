{
  "name": "agora-localnet-dev-env",
  "version": "1.0.0",
  "description": "Local network development environment",
  "scripts": {
    "build": "hardhat compile",
    "deploy": "hardhat deploy --network localhost",
    "deploy:commons_budeget": "hardhat run scripts/deploy_commons_budget.ts --network localhost",
